<?xml version="1.0" encoding="UTF-8"?>
<LinearLayout
    view_width="full"
    view_height="full"
    view_orientation="V"
    view_background="#fff0f0f0">
    <Navigation
        view_background="#ffffffff">
        
    </Navigation>
    
    <CustomView
        view_name="selector_profile"
        view_class="SelectorItem"
        view_width="full"
        view_height="48sp"
        layout_marginTop="15"
        layout_marginBottom="15"
        selector_leftText="个人信息"/>
        
    <CustomView
        view_name="selector_feedback"
        view_class="SelectorItem"
        view_width="full"
        view_height="48sp"
        selector_leftText="意见反馈"/>
    <CustomView
        view_name="selector_contact"
        view_class="SelectorItem"
        view_width="full"
        view_height="48sp"
        selector_leftText="联系客服"/>
    <CustomView
        view_class="SelectorItem"
        view_width="full"
        view_height="48sp"
        layout_marginBottom="15"
        selector_leftText="商家入驻"/>
        
    <CustomView
        view_class="SelectorItem"
        view_width="full"
        view_height="48sp"
        selector_leftText="清理缓存"
        selector_arrow="NO"/>
    <CustomView
        view_class="SelectorItem"
        view_width="full"
        view_height="48sp"
        selector_leftText="关于我们"
        selector_rightText="最新版本"/>
        
        
    <Button
        view_name="btn_code"
        view_width="full"
        view_height="40"
        view_background="#cccccc"
        view_backgroundSelected="#22000000"
        view_borderRadius="5"
        view_masksToBounds="YES"
        text_src="退出登录"
        text_size="14"
        text_color="#ffffff"
        layout_marginTop="100"
        layout_marginLeft="77"
        layout_marginRight="77"/>
    
</LinearLayout>
