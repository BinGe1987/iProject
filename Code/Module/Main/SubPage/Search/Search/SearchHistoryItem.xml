<?xml version="1.0" encoding="UTF-8" ?>
<FrameLayout
    view_name="layout_history"
    view_width="full"
    view_height="36sp">
    <LinearLayout
        view_width="full"
        view_height="full"
        view_orientation="horizontal">
        <ImageView
            view_width="auto"
            view_height="full"
            image_src="@icon_history"
            layout_marginLeft="10">
        </ImageView>
        <Label
            view_name="label_history"
            view_width="auto"
            view_height="full"
            text_src="搜索记录1"
            text_color="#7e7e7e"
            text_size="14"
            layout_marginLeft="10">
        </Label>
    </LinearLayout>
    <View
        view_width="full"
        view_height="0.5"
        view_background="#f4f4f4"
        layout_gravity="bottom">
    </View>
    <Button
        view_name="btn_item"
        view_width="full"
        view_height="full"
        view_background="#00ffffff"
        view_backgroundSelected="#22000000">
    </Button>
</FrameLayout>


