<?xml version="1.0" encoding="UTF-8"?>
<LinearLayout
    view_width="full"
    view_height="full"
    view_orientation="V"
    view_background="#fff0f0f0">
    <Navigation
        view_background="#ffffffff">
        
    </Navigation>
    
    <FrameLayout
        view_width="full"
        view_height="52sp"
        layout_marginTop="15"
        layout_marginBottom="15">
        <CustomView
            view_name="selector_head"
            view_class="SelectorItem"
            view_width="full"
            view_height="full"
            selector_leftText="头像"/>
        <ImageView
            view_name="image_head"
            view_width="36sp"
            view_height="36sp"
            view_borderRadius="18sp"
            view_masksToBounds="YES"
            layout_gravity="center|right"
            layout_marginRight="40"/>
    </FrameLayout>
    
    
    <CustomView
        view_name="selector_nicename"
        view_class="SelectorItem"
        view_width="full"
        view_height="48sp"
        selector_leftText="昵称"/>
    <CustomView
        view_name="selector_gender"
        view_class="SelectorItem"
        view_width="full"
        view_height="48sp"
        selector_leftText="性别"/>
    <CustomView
        view_name="selector_phone"
        view_class="SelectorItem"
        view_width="full"
        view_height="48sp"
        selector_leftText="手机"/>
    
</LinearLayout>
