<?xml version="1.0" encoding="UTF-8"?>
<FrameLayout
    view_name="page_tech"
    view_width="full"
    view_height="full">
    
    <CollectionView
        view_name="collection"
        view_width="full"
        view_height="full"
        view_background="#ffffff"/>
    
    <Button
        view_name="btn_techHot"
        view_type="button"
        view_width="86"
        view_height="24"
        view_background="#ffd1eff2"
        view_backgroundSelected="#ff4abdcf"
        view_borderRadius="12"
        view_masksToBounds="YES"
        text_src="热门技师"
        text_colorSelected="#ffffff"
        text_color="4abdcf"
        layout_gravity="center|bottom"
        layout_marginBottom="15"
        layout_marginRight="55"/>
    
    <Button
        view_name="btn_techTop"
        view_type="button"
        view_width="86"
        view_height="24"
        view_background="#ffd1eff2"
        view_backgroundSelected="#ff4abdcf"
        view_borderRadius="12"
        view_masksToBounds="YES"
        text_src="头牌技师"
        text_colorSelected="#ffffff"
        text_color="#4abdcf"
        layout_gravity="center|bottom"
        layout_marginBottom="15"
        layout_marginLeft="55"/>
    
</FrameLayout>


